# Backend API URL (pointing to your local FastAPI server)
REACT_APP_BACKEND_URL=http://localhost:8000
REACT_APP_GOOGLE_AUTH_URL=http://localhost:8000/auth/google
# Enable visual edits in development (optional)
REACT_APP_ENABLE_VISUAL_EDITS=true

# Health check (optional, can be false)
ENABLE_HEALTH_CHECK=false